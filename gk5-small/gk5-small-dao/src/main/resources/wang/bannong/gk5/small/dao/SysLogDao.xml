<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="wang.bannong.gk5.small.dao.SysLogDao">

    <select id="selectSysLogPage" resultType="wang.bannong.gk5.small.common.entity.SysLogEntity">
        select * from sys_log where 1=1
        <if test="params.key != null and params.key.trim() != ''">
            and `username` like '%${params.key}%' or `operation` like '%${params.key}%'
        </if>
    </select>

</mapper>