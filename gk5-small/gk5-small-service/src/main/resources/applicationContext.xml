<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.1.xsd">

    <!-- properties配置文件加载 -->
    <context:property-placeholder location="classpath*:caffeine.properties"/>
    <context:property-placeholder location="classpath*:generator.properties"/>
    <context:property-placeholder location="classpath*:j2cache.properties"/>
    <context:property-placeholder location="classpath*:platform.properties"/>
    <!--<import resource="platform-scheduler.xml"/>-->
</beans>
